<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.megamind.viewrootimpl">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ChangeWithSleepFixedActivity"/>
    <activity android:name=".ChangeWithSleepWrapActivity"/>
    <activity android:name=".OnChildThreadActivity"/>
    <activity android:name=".OnClickActivity"/>
    <activity android:name=".WrapTextActivity"/>
    <activity android:name=".RequestBeforeActivity"/>
    <activity android:name=".FirstSetTextInMainThreadWrapTextActivity"/>
    <activity android:name=".AddViewWithoutMainThread"/>
  </application>

</manifest>